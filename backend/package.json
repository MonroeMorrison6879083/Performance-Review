{
  "name": "performance-review-backend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "clean": "rimraf cache artifacts deployments",
    "node": "hardhat node",
    "compile": "hardhat compile",
    "deploy": "hardhat deploy --network localhost",
    "deploy:sepolia": "hardhat deploy --network sepolia",
    "export-frontend": "node scripts/export-frontend.js",
    "dev:all": "run-p node \"wait-on tcp:8545 && yarn deploy && yarn export-frontend\""
  },
  "dependencies": {
    "@fhevm/solidity": "^0.9.1"
  },
  "devDependencies": {
    "@fhevm/hardhat-plugin": "^0.3.0-1",
    "@zama-fhe/relayer-sdk": "^0.3.0-5",
    "ethers": "^6.13.2",
    "hardhat": "^2.22.12",
    "hardhat-deploy": "^0.12.4",
    "hardhat-deploy-ethers": "^0.4.2",
    "dotenv": "^16.4.5",
    "rimraf": "^6.0.1",
    "npm-run-all": "^4.1.5",
    "wait-on": "^7.2.0"
  }
}


